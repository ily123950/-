local tabContents = {}
local walkspeedEnabled = false
local walkspeedConnection = nil

local function createTabContent(name)
	local contentFrame = Instance.new("Frame")
	contentFrame.Size = UDim2.new(0.65, -10, 1, -80)
	contentFrame.Position = UDim2.new(0.3, 10, 0, 80)
	contentFrame.BackgroundTransparency = 1
	contentFrame.Visible = false
	contentFrame.Parent = mainFrame

	if name == "Main" then
		local actionButton = Instance.new("TextButton")
		actionButton.Size = UDim2.new(0, 150, 0, 45)
		actionButton.Position = UDim2.new(0, 10, 0, 10)
		actionButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
		actionButton.Text = "Enable Walkspeed"
		actionButton.TextColor3 = Color3.fromRGB(255, 255, 255)
		actionButton.TextScaled = true
		actionButton.Font = Enum.Font.Gotham
		actionButton.Parent = contentFrame
		local buttonCorner = Instance.new("UICorner", actionButton)
		buttonCorner.CornerRadius = UDim.new(0, 8)

		actionButton.MouseButton1Click:Connect(function()
			if not walkspeedEnabled then
				local character = player.Character or player.CharacterAdded:Wait()
				local root = character:WaitForChild("HumanoidRootPart")
				local humanoid = character:WaitForChild("Humanoid")

				local SPEED = 105
				local FORCE_MULTIPLIER = 90
				local STOP_MULTIPLIER = 300

				local function tryEquipAndUseTool()
					local tool = player.Backpack:FindFirstChild("Invisibility Cloak")
					if tool and tool:IsA("Tool") then
						tool.Parent = character
						task.wait(0.1)
						tool:Activate()
					else
						StarterGui:SetCore("SendNotification", {
							Title = "Инструмент не найден",
							Text = "У тебя нет Invisibility Cloak в рюкзаке!",
							Duration = 5
						})
					end
				end

				tryEquipAndUseTool()

				local attachment = Instance.new("Attachment", root)
				local vectorForce = Instance.new("VectorForce")
				vectorForce.Name = "ConstantMoveForce"
				vectorForce.Attachment0 = attachment
				vectorForce.RelativeTo = Enum.ActuatorRelativeTo.World
				vectorForce.ApplyAtCenterOfMass = true
				vectorForce.Force = Vector3.zero
				vectorForce.Parent = root

				walkspeedConnection = RunService.RenderStepped:Connect(function()
					local moveDir = humanoid.MoveDirection
					local currentVelocity = root.Velocity
					local flatVelocity = Vector3.new(currentVelocity.X, 0, currentVelocity.Z)

					if moveDir.Magnitude > 0 then
						local targetVelocity = moveDir.Unit * SPEED
						local difference = targetVelocity - flatVelocity
						vectorForce.Force = difference * FORCE_MULTIPLIER
					else
						vectorForce.Force = -flatVelocity * STOP_MULTIPLIER
					end
				end)

				actionButton.Text = "Disable Walkspeed"
				walkspeedEnabled = true
			else
				if walkspeedConnection then
					walkspeedConnection:Disconnect()
					walkspeedConnection = nil
				end
				local character = player.Character
				if character then
					local root = character:FindFirstChild("HumanoidRootPart")
					if root then
						local vectorForce = root:FindFirstChild("ConstantMoveForce")
						if vectorForce then
							vectorForce:Destroy()
						end
						local attachment = root:FindFirstChildOfClass("Attachment")
						if attachment then
							attachment:Destroy()
						end
					end
				end
				actionButton.Text = "Enable Walkspeed"
				walkspeedEnabled = false
			end
		end)
	else
		local actionButton = Instance.new("TextButton")
		actionButton.Size = UDim2.new(0, 100, 0, 45)
		actionButton.Position = UDim2.new(0, 10, 0, 10)
		actionButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
		actionButton.Text = "Действие " .. name
		actionButton.TextColor3 = Color3.fromRGB(255, 255, 255)
		actionButton.TextScaled = true
		actionButton.Font = Enum.Font.Gotham
		actionButton.Parent = contentFrame
		local buttonCorner = Instance.new("UICorner", actionButton)
		buttonCorner.CornerRadius = UDim.new(0, 8)
	end

	return contentFrame
end

tabContents["Main"] = createTabContent("Main")
tabContents["Настройки"] = createTabContent("Настройки")
tabContents["Информация"] = createTabContent("Информация")

local currentTab = "Main"
